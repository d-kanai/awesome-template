{
  "name": "frontend_native",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "start": "expo start",
    "start:local": "dotenv -e .env.local -- expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "dotenv -e .env.local -- expo start --android",
    "ios": "dotenv -e .env.local -- expo start --ios",
    "web": "expo start --web",
    "lint": "biome check --write",
    "lint:check": "biome check",
    "format": "biome format --write",
    "format:check": "biome format --check",
    "generate:api": "orval --config orval.config.cjs",
    "typecheck": "tsc --noEmit",
    "test": "jest --config jest.config.js",
    "test:coverage": "jest --config jest.config.js --coverage",
    "test:e2e": "maestro test e2e/features/auth/signup.yaml --config e2e/config.yaml --env BACKEND_URL=http://localhost:8080 --env WAIT_TIMEOUT=2000"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@tanstack/react-form": "^1.23.8",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/zod-form-adapter": "^0.42.1",
    "expo": "~54.0.20",
    "expo-constants": "~18.0.10",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.13",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.8.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.0",
    "dotenv-cli": "^11.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.13",
    "orval": "^7.1.1",
    "react-test-renderer": "19.1.0",
    "start-server-and-test": "^2.1.2",
    "typescript": "~5.9.2"
  },
  "engines": {
    "node": ">=22.11.0 <23"
  },
  "private": true
}
